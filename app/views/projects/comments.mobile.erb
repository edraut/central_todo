<div id="pager_wrapper" class="pager_wrapper page_slide">
<%= render :partial => 'comments', :locals => {:project => @project} %>
<%= javascript_tag do %>
jQuery(document).ready(function(){
	children_pages = eval(jQuery('#comments_project_page_<%= @project.id %>').attr('data-children_pages'));
	loadPages(children_pages);
	jQuery('#comments_project_page_<%= @project.id %>').addClass('current_page');
  jQuery('#pager_wrapper').data('loaded_page_urls')['<%= request.url %>'] = 'comments_project_page_<%= @project.id %>'
  jQuery('#pager_wrapper').data('loaded_page_ids')['comments_project_page_<%= @project.id %>'] = '<%= request.url %>'
  jQuery('#pager_wrapper').data('back_id','comments_project_page_<%= @project.id %>');
});
<% end %>