<%= form_for project, :url => plan_url(project), :html => {
	:'data-ajax_behavior' => 'ajax_form',
	:'data-ajax_method' => 'PUT',
	:'data-ajax_success_element' => "project_done",
	:id => 'edit_project_done'} do |f| %>
	<%= f.check_box :state, {:'data-behavior' => 'auto_submit', :'data-role' => 'none', :id => 'project_state_checkbox'}, 'archived', 'active' %>
  <%= hidden_field_tag 'attribute','done', :id => 'attribute_done_field' %>
  archived
<% end %>