<div id="project_title" class="borderless section">
	<%= render :partial => 'show_title' %>
</div>
<%= render :partial => 'tabs' %>
<div class="section controls">
  <div id="project_delete" class="row_item list_action last"><%= render :partial => 'show_delete', :locals => {:project => @project} %></div>
  <% if @project.all_complete %>
  <div id="project_done" class="row_item last"><%= render :partial => 'show_done', :locals => {:project => @project} %></div>
  <% else %>
  <div id="project_done" class="row_item last text_with_check_box_proxy">-in progress</div>
  <% end %>
</div>
<div id="project_description" class="section">
	<%= render :partial => @project.description.blank? ? 'edit_description' : 'show_description' %>
</div>
<div id="project_due_date" class="section">
	<%= render :partial => @project.due_date.blank? ? 'edit_due_date' : 'show_due_date' %>
</div>
