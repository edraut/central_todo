<div class="span-18 last section_margin_top" id="header_wrapper">
  <div class="span-3 header_wrapper"><h1>Plan:</h1></div>
  <div class="span-15 last item_controls">
    <div id="show_delete" class="list_action attach_above last"><%= render :partial => 'show_delete', :locals => {:project => @project} %></div>
    <div id="show_done" class="list_action attach_above"><%= render :partial => 'show_done', :locals => {:project => @project} %></div>
    <div id="to_task_list" class="list_action attach_above">&larr; <%= link_to 'back to task list', plan_url(@project) %></div>
  </div>
</div>
<div class="span-19 last page_content">
  <div id="project_title" class="span-18 last">
  	<%= render :partial => 'show_title' %>
  </div>
  <div id="project_description" class="span-18 last section_margin_top">
  	<%= render :partial => 'show_description' %>
  </div>
  <div id="project_due_date" class="span-18 last section_margin_top">
  	<%= render :partial => 'show_due_date' %>
  </div>
</div>