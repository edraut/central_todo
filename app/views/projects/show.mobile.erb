<div id="pager_wrapper" class="pager_wrapper page_slide">
<%= render :partial => 'show_full' %>
<%= javascript_tag do %>
jQuery(document).ready(function(){
	children_pages = eval(jQuery('#project_page_<%= @project.id %>').attr('data-children_pages'));
	loadPages(children_pages);
	jQuery('#project_page_<%= @project.id %>').addClass('current_page');
  jQuery('#pager_wrapper').data('loaded_page_urls')['<%= request.url %>'] = 'project_page_<%= @project.id %>'
  jQuery('#pager_wrapper').data('loaded_page_ids')['project_page_<%= @project.id %>'] = '<%= request.url %>'
});
<% end %>