<div id="pager_wrapper" class="pager_wrapper page_slide">
  <%= render :partial => 'index' %>
</div>
<%= javascript_tag do %>
jQuery(document).ready(function(){
	children_pages = eval(jQuery('#dashboard_overview').attr('data-children_pages'));
	loadPages(children_pages);
	jQuery('#dashboard_overview').addClass('current_page');
  jQuery('#pager_wrapper').data('loaded_page_urls')['<%= request.url %>'] = 'dashboard_overview'
  jQuery('#pager_wrapper').data('loaded_page_ids')['dashboard_overview'] = '<%= request.url %>'
});
<% end %>