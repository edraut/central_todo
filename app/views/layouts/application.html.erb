                </div>
              <% end %>
						  <% for contact in User.contacts_for(@this_user) %>
					      <%  selected = (@nav_tab == 'contacts' and @user and !@project and @user.id == @subnav_tab and @user.id == contact.id)
					          super_selected = (@nav_tab == 'contacts' and @project and (@project.user_id == contact.id or @project.project_sharers.detect{|ps| ps.user_id = contact.id})) %>
                <div id="contact_<%= contact.id %>_nav" class="expandable_subnav" data-sort_element="true" data-sort_id="contact_list">
                  <div class="expandable_subnav <% if selected %> selected<% end %>" data-binding="hover_show_more" data-hover_div="[data-hover_id='contact_nav_<%= contact.id %>']" data-hover_fade_time="50" data-hover_fadein_time="50">
    						    <div class="expand_arrow_width" data-behavior="expander" data-action="expand" data-state="contracted" data-id="contact_<%= contact.id %>" <% if selected or super_selected %>style="display:none;"<% end %>>&nbsp;&gt;</div>
    						    <div class="expand_arrow_width" data-behavior="expander" data-action="contract" data-state="expanded" data-id="contact_<%= contact.id %>" <% unless selected or super_selected %>style="display:none;"<% end %>>&nbsp;&or;</div>
    						    <div class="expanded_subnav">
    						      <div class="overflow_fade">&nbsp;</div>
                      <div data-drag_handle="true" class="allcaps expandable_subnav_stretcher">
                        &nbsp;<a href="<%= user_url(contact) %>" data-resource_attr="contact_title" data-resource_id="<%= contact.id %>"><%= contact.handle %></a>
                      </div>
                		</div>
                	</div>
      						<div class="expandable_subnav" id="contact_<%= contact.id %>_plan_list_nav" <% unless selected or super_selected %>style="display:none;"<% end %> data-behavior="expander"  data-state="expanded" data-id="contact_<%= contact.id %>">
      						  <%  @contact_projects = (@this_user.shared_projects.active.where("projects.user_id = :user_id",{:user_id => contact.id}) + contact.shared_projects.active.where("projects.user_id = :user_id",{:user_id => @this_user.id})).uniq
      						      unless @contact_projects.any?
                          @contact_projects = (@this_user.shared_projects.where("projects.user_id = :user_id",{:user_id => contact.id}) + contact.shared_projects.where("projects.user_id = :user_id",{:user_id => @this_user.id})).uniq
    						        end
      						      for project in @contact_projects %>
