        							<div class="expandable_subnav">
                        <div class="expanded_subnav_indent<% if @subnav_tab == 'archived' %> selected<% end %>">
        								  &nbsp;<%= link_to "Archived Plans", archived_plans_url %>
        								</div>
        							</div>
        							<% end %>
        						  <% if @this_user.plan_templates.any? %>
