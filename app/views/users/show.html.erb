<div class="span-16 last section_margin_top" id="header_wrapper">
<h1>Contact</h1>
</div>
<div class="span-17 last page_content">
  <% if @projects_shared_to.any? %>
  <h2>Plans You Shared with <%= @user.handle %></h2>
  <div id="shared_to_plan_list" class="span-16 last list_section">
    <% for project in @projects_shared_to %>
    <div id="project_<%= project.id %>" class="span-17 last" data-role="list_item">
			<%= render :partial => '/projects/show', :locals => {:project => project, :sortable => true, :hierarchical => true} %>
		</div>
  	<% end %>
	</div>
	<% end %>
  <% if @projects_shared_from.any? %>
  <div class="span-16 section_margin_top">
    <h2>Plans <%= @user.handle %> Shared with You</h2>
    <div id="shared_from_plan_list" class="span-16 last list_section">
      <% for project in @projects_shared_from %>
      <div id="project_<%= project.id %>" class="span-17 last" data-role="list_item">
  			<%= render :partial => '/projects/show', :locals => {:project => project, :sortable => true, :hierarchical => true} %>
  		</div>
    	<% end %>
  	</div>
	</div>
	<% end %>
</div>