<div id="pager_wrapper" class="pager_wrapper page_slide">
<%= render :partial => 'show_full' %>
<%= javascript_tag do %>
jQuery(document).ready(function(){
	children_pages = eval(jQuery('#contact_page_<%= @user.id %>').attr('data-children_pages'));
	loadPages(children_pages);
	jQuery('#contact_page_<%= @user.id %>').addClass('current_page');
  jQuery('#pager_wrapper').data('loaded_page_urls')['<%= request.url %>'] = 'contact_page_<%= @user.id %>'
  jQuery('#pager_wrapper').data('loaded_page_ids')['contact_page_<%= @user.id %>'] = '<%= request.url %>'
});
<% end %>