<div id="pager_wrapper" class="pager_wrapper page_slide">
<%= render :partial => 'show_full' %>
<%= javascript_tag do %>
jQuery(document).ready(function(){
	children_pages = eval(jQuery('#label_page_<%= @label.id %>').attr('data-children_pages'));
	loadPages(children_pages);
	jQuery('#label_page_<%= @label.id %>').addClass('current_page');
  jQuery('#pager_wrapper').data('loaded_page_urls')['<%= request.url %>'] = 'label_page_<%= @label.id %>'
  jQuery('#pager_wrapper').data('loaded_page_ids')['label_page_<%= @label.id %>'] = '<%= request.url %>'
  jQuery('#pager_wrapper').data('back_id','label_page_<%= @label.id %>');
});
<% end %>