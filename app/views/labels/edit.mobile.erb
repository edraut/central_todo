<div id="pager_wrapper" class="pager_wrapper page_slide">
<%= render :partial => 'edit' %>
<%= javascript_tag do %>
jQuery(document).ready(function(){
	children_pages = eval(jQuery('#info_label_page_<%= @label.id %>').attr('data-children_pages'));
	loadPages(children_pages);
	jQuery('#info_label_page_<%= @label.id %>').addClass('current_page');
  jQuery('#pager_wrapper').data('loaded_page_urls')['<%= request.url %>'] = 'info_label_page_<%= @label.id %>'
  jQuery('#pager_wrapper').data('loaded_page_ids')['info_label_page_<%= @label.id %>'] = '<%= request.url %>'
});
<% end %>
