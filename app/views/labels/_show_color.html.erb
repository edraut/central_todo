<div class="span-3">
<h2>Color</h2>
</div>
<div class="span-13 last">
  <div class="list_action">
		<%= link_to 'edit color', edit_label_url(@label, :attribute => 'color'),
		 	:'data-ajax_behavior' => 'ajax_link',
			:'data-ajax_method' => 'GET',
			:'data-ajax_success_element' => 'label_color'
		%>
  </div>
</div>
<div class="span-16 last section">
  <div class="label <%= @label.color %>"><%= @label.color.humanize.downcase %></div>
</div>
