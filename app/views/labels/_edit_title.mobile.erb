<h2>Label:</h2>
<%= form_for @label, :html => {
	:'data-ajax_behavior' => 'ajax_form',
	:'data-ajax_method' => 'PUT',
	:'data-ajax_success_element' => 'label_title',
	:'data-ajax_error_element' => 'label_title'
} do |f| %>
	<%= f.text_field :title %>
	<%= hidden_field_tag 'attribute', 'title' %>
	<div class="row_item last padding_top_button small list_action">&nbsp;or
	<span href="<%= label_url(@label) %>"
	 	data-ajax_behavior="ajax_link"
		data-ajax_method="GET"
		data-ajax_success_element="label_title"
		data-ajax_data="{attribute: 'title'}"%>(cancel)</span>
	</div>
	<div onclick="void(0)" data-button="true" data-submit="true" class="row_item button round small light "><div class="button_content">save title</div></div>
<% end %>
