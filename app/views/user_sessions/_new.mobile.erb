  <h1>Login</h1>
  <div class="">
  	<% form_for :user_session, @user_session, :url => user_session_url do |f| %>
  	  <% if @user_session.errors.any? %> <%= @user_session.errors.full_messages %><br/> <% end %>
  	  <%= f.label :email %><br />
  	  <%= f.text_field :email %><br />
  	  <br />
  	  <%= f.label :password %><br />
  	  <%= f.password_field :password %><br />
  	  <br />
  	  <%= f.submit "Login" %>
  	<% end %>
  	<%= link_to 'Forget your password?', forgot_password_user_url('none') %>
  </div>
