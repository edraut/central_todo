<div class="span-17 last section_margin_top" id="header_wrapper">
	<h1>Archived Unorganized Tasks</h1>
</div>
<div class="span-17 last page_content">
  <% if @tasks.any? %>
  <div id="archived_item_heading" class="span-16 last">
		<h2>You got these done.</h2>
	</div>
	<div id="archived_items_wrapper" class="span-16 last archived list_section" data-role="list">
    <div class="span-16 last line_item">
      <div class="span-9 append-3 text_center small">task title</div>
      <div class="span-3 small">&nbsp;&nbsp;plan</div>
    </div>
		<% for task in @tasks %>
			<div class="span-16 last" id="task_<%= task.id %>" data-role="list_item">
				<%= render :partial => '/tasks/show', :locals => {:task => task} %>
			</div>
		<% end %>
	</div>
	<div class="span-17">
	<%= will_paginate @tasks %>
	</div>
	<% else %>
	You don't have any archived unorganized tasks right now.
	<% end %>
</div>